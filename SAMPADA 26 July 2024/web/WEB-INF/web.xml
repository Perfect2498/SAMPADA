<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    
     <servlet>
        <servlet-name>CaptchaGenServlet</servlet-name>
        <servlet-class>sampada.util.CaptchaGenServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AvailableBills</servlet-name>
        <servlet-class>com.AvailableBills</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckBankStmtExistance</servlet-name>
        <servlet-class>com.CheckBankStmtExistance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchWeekInfoServlet</servlet-name>
        <servlet-class>com.FetchWeekInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckPendingPayableBillCorporateWise</servlet-name>
        <servlet-class>com.CheckPendingPayableBillCorporateWise</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckUniqueCommercialGrpShortName</servlet-name>
        <servlet-class>com.CheckUniqueCommercialGrpShortName</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckUniqueEntityShortName</servlet-name>
        <servlet-class>com.CheckUniqueEntityShortName</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AvailableBills</servlet-name>
        <url-pattern>/AvailableBills</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckBankStmtExistance</servlet-name>
        <url-pattern>/CheckBankStmtExistance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FetchWeekInfoServlet</servlet-name>
        <url-pattern>/FetchWeekInfoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckPendingPayableBillCorporateWise</servlet-name>
        <url-pattern>/CheckPendingPayableBillCorporateWise</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckUniqueCommercialGrpShortName</servlet-name>
        <url-pattern>/CheckUniqueCommercialGrpShortName</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckUniqueEntityShortName</servlet-name>
        <url-pattern>/CheckUniqueEntityShortName</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>CaptchaGenServlet</servlet-name>
        <url-pattern>/captcha.jpg</url-pattern>
    </servlet-mapping> 
    <session-config>
        <session-timeout>
           30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>welcomeSAMPADA.jsp</welcome-file>
    </welcome-file-list>
</web-app>
